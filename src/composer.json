{
    "name": "kmuenkel/conduit",
    "description": "Easily produce Adapter classes and dummy-responses for testing them",
    "time": "2020-01-29 17:31:00",
    "authors": [
        {
            "name": "Kevin Muenkel",
            "email": "kevin_tm@yahoo.com"
        }
    ],
    "keywords": [
        "testing",
        "generator",
        "adapter",
        "laravel",
        "API"
    ],
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": ">=6.0",
        "kmuenkel/handler-stack": "^1.0",
        "kmuenkel/rule-evaluator": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Concept\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "Conduit\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "license": "MIT",
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Conduit\\Providers\\ConduitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
